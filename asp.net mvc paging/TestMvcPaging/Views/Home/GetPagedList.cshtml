@model List<MyData>
    @{
        Layout = null;
    }
<script>
    var total = @ViewData["TotalRow"];
    var pindex=@ViewData["PageIndex"];
    var pagesize=@ViewData["PageSize"];

    //var content = '@ViewData["content"]';
   
    $('#pageDiv').setTotalRow(total,pindex,pagesize);
</script>
 
@if (Model != null && Model.Count > 0)
{
    <table class="table table-bordered table-responsive">

        <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
            <th>Mail</th>
            <th>Title</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Address</td>
                <td>@item.Mail</td>
                <td>@item.Title</td>
            </tr>
        }
        </tbody>
    </table>

}
